{
  "name": "@ryantate/js-cli",
  "version": "1.1.4",
  "bin": {
    "js": "cli.js"
  },
  "description": "Quickly pipe stdin from scripts to a JS lambda or expression",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryantate13/js-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "dependencies": {
    "@ltd/j-toml": "^1.35.2",
    "json-colorizer": "^2.2.2",
    "mock-stdin": "^1.0.0",
    "papaparse": "^5.3.2",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@types/papaparse": "^5.3.5",
    "jest": "^29.2.0",
    "jest-mock-console": "^2.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "test": "jest --coverage --verbose --colors",
    "test:dev": "jest --coverage --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts",
    "collectCoverageFrom": [
      "**/*.ts"
    ]
  }
}
