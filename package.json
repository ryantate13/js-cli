{
  "name": "@ryantate/js-cli",
  "version": "1.1.3",
  "bin": {
    "js": "cli.js"
  },
  "description": "Quickly pipe stdin from scripts to a JS lambda or expression",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryantate13/js-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "dependencies": {
    "@ltd/j-toml": "^1.20.0",
    "json-colorizer": "^2.2.2",
    "mock-stdin": "^1.0.0",
    "papaparse": "^5.3.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.9",
    "@types/papaparse": "^5.3.0",
    "jest": "^27.2.5",
    "jest-mock-console": "^1.2.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "test": "jest --coverage --verbose --colors",
    "test:dev": "jest --coverage --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": ".*\\.test\\.ts",
    "collectCoverageFrom": [
      "**/*.ts"
    ]
  }
}
