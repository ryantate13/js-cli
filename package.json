  },
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "dependencies": {
    "json-colorizer": "^2.2.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "jest": "^26.6.3",
    "jest-mock-console": "^1.0.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "test": "jest --coverage --verbose",
    "test:dev": "jest --coverage --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "clearMocks": true,
    "coverageDirectory": "coverage",
